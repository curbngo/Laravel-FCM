{
    "name": "curbngo/laravel-fcm",
    "description": "Laravel / Lumen package for Firebase Cloud Messaging ",
    "keywords": [
        "laravel",
        "lumen",
        "firebase",
        "notification",
        "push",
        "fcm",
        "firebase cloud messaging"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Brosy",
            "email": "nicolas.brosy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "guzzlehttp/guzzle": "~7.0",
        "monolog/monolog": "^1.12|^2.0|^3.0",
        "google/auth": "^1.37"
    },
    "require-dev": {
        "laravel/laravel": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFCM\\": "src/",
            "LaravelFCM\\Mocks\\": "tests/mocks"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFCM\\FCMServiceProvider"
            ],
            "aliases": {
                "FCM": "LaravelFCM\\Facades\\FCM"
            }
        }
    }
}
